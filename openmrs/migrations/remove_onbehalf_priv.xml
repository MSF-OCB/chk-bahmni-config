<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
        <changeSet id="201802100516" author="vishnu"  >
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="4">
                select count(*) from role_privilege where privilege  in ('app:clinical:onbehalf');
            </sqlCheck>
        </preConditions>
        <comment>Removing the onbehalf privilege from bahmni</comment>
        <sql>
        delete from role_privilege where privilege='app:clinical:onbehalf';
        </sql>
        </changeSet>
        </databaseChangeLog>
